  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "start": "node index.ts",
    "dev": "tsx watch index.ts",
    "db:create": "cat ./data/create_table.sql | docker exec -i 'petfoster-db' psql -U petfoster_admin -d petfoster",
    "db:populate": "cat ./data/populate_table.sql | docker exec -i 'petfoster-db' psql -U petfoster_admin -d petfoster",
    "db:reset": "pnpm run db:create && pnpm run db:populate",
    "compose:dev": "docker compose -f docker-compose.dev.yml up --build",
    "rm:container": "docker rm -v -f petfoster-db petfoster-api",
    "container:api": "docker exec -it 'petfoster-api' bash",
    "container:db": "docker exec -it 'petfoster-db' bash"
  },
